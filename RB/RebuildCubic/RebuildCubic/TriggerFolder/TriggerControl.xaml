<UserControl
  x:Class="RebuildCubic.TriggerFolder.TriggerControl"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:InventoryFolder="clr-namespace:RebuildCubic.InventoryFolder"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  d:DesignHeight="450"
  d:DesignWidth="800"
  mc:Ignorable="d"
  >
  <Grid>
    <Button
      x:Name="TriggerButton"
      Width="100"
      Height="100"
      Margin="670,150,0,0"
      HorizontalAlignment="Left"
      VerticalAlignment="Top"
      Content="Trigger"
      />
    <InventoryFolder:InventoryControl x:Name="inventorycontorl" />
  </Grid>
</UserControl>
