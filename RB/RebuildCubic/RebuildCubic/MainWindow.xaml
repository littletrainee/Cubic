<Window
  x:Class="RebuildCubic.MainWindow"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:ConnectFolder="clr-namespace:RebuildCubic.ConnectFolder"
  xmlns:DataTableFolder="clr-namespace:RebuildCubic.DataTableFolder"
  xmlns:InventoryFolder="clr-namespace:RebuildCubic.InventoryFolder"
  xmlns:StateFolder="clr-namespace:RebuildCubic.StateFolder"
  xmlns:TriggerFolder="clr-namespace:RebuildCubic.TriggerFolder"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  Title="RFID Reader By Glue-Soft"
  Width="800"
  Height="450"
  mc:Ignorable="d"
  >
  <Window.DataContext>
    <ConnectFolder:ChangeState />
  </Window.DataContext>
  <Grid>
    <ConnectFolder:ConnectControl />
    <StateFolder:StateControl />
    <TriggerFolder:TriggerControl />
    <InventoryFolder:InventoryControl />
    <DataTableFolder:DataTableControl />

    <!-- <ClearFolder:ClearControl /> -->
    <!-- <SetFolder:SetControl /> -->


  </Grid>
</Window>
